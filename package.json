  },
  "dependencies": {
    "colors": "^1.1.2",
    "yamljs": "^0.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jerrywdlee/dev-service-runner.git"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^4.0.1",
    "nyc": "^11.4.1",
    "power-assert": "^1.4.4"
  }
}
